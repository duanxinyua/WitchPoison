# 女巫的毒药 - 前端环境配置示例
# 复制此文件为 .env 并填入实际配置值

# 应用基本信息
VUE_APP_NAME=女巫的毒药
VUE_APP_VERSION=1.0.0
VUE_APP_DESCRIPTION=女巫的毒药

# 微信小程序配置
VUE_APP_WECHAT_APPID=your_wechat_appid_here

# 后端服务配置
VUE_APP_API_BASE_URL=https://your-domain.com
VUE_APP_WS_URL=wss://your-domain.com

# 开发环境配置（如需要）
VUE_APP_DEV_API_BASE_URL=http://localhost:3000
VUE_APP_DEV_WS_URL=ws://localhost:3000

# UniApp应用ID
VUE_APP_UNI_APPID=your_uni_appid_here